<%- include("partials/header") %>
<div class="container"  >
    <form>
        <div class="form-group">
            <h1> REGISTER </h1>
            <label id="spans">FIRSTNAME : </label>
            <p></p>
            <input class="form-control" type="text" name="user" id="firstname" placeholder="firstname" required="MUST AND SHOULD REQUIRED">
            <p></p>
            <label id="spans">LASTNAME : </label>
            <p></p>
            <input class="form-control" type="text" name="last" id="lastname" placeholder="lastname" required="MUST AND SHOULD REQUIRED">
        </div>
        <div class="form-group">
            <p></p>
            <span id="spans">EMAIL : </span>
            <p></p>
            <input class="form-control" type="email" name="email" id="email" placeholder="@gmail.com" required="@gmail format">
        </div>
        <div class="form-group">
            <label id="spans">Complaint:</label>
            <textarea class="form-control" type="text"></textarea>
        </div>
        <div class="form-group">
            <label id="spans">FeedBack :</label>
            <input class="form-control" type="text">
        </div>
        <input class="btn-primary" type="submit" name="submit" id="submit">
</form>
</div>
<%- include("partials/footer") %>